<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Date</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @report.each do |e| %>
      <tr>
        <td><%= e.report_type %></td>
        <td><%= e.date %></td>
        <td><%= e.description %></td>
        <td><%= link_to 'Edit', edit_report_path(e) %></td>
        <td><%= link_to 'Destroy', e, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <td><%= link_to 'New Report', new_report_path %></td>
  </tbody>
</table>